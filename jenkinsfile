pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('Checkout'){
            git url:'https://github.com/nitish-002/jk-pra.git', branch:'main'
            echo "Checkout complete"
        }
        stage('Build'){
            sh 'mvn clean package'
        }
        stage('Test'){
            sh 'mvn test'
        }
    }
}
